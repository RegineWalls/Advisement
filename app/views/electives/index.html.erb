<h1>Listing electives</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @electives.each do |elective| %>
      <tr>
        <td><%= link_to 'Show', elective %></td>
        <td><%= link_to 'Edit', edit_elective_path(elective) %></td>
        <td><%= link_to 'Destroy', elective, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Elective', new_elective_path %>
